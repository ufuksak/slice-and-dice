    "dotenv": "^16.0.3",
    "esw": "^0.15.0",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.5",
    "sql.js": "^1.8.0",
    "sqlite3": "^5.1.2",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.10",
    "typeorm-test-transactions": "^3.6.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/helmet": "^4.0.0",
    "@types/ioredis": "^4.28.10",
