{
  "name": "slice-and-dice",
  "version": "1.2.0",
  "description": "Clean, simple, agnostic REST API scaffold; built with TypeScript, Express, Passport, JWT, Redis, TypeORM and all the good stuff.",
  "main": "dist/server.js",
  "scripts": {
    "build:dev": "tsc --watch",
    "start": "ts-node src/server.ts",
    "start:dev": "ts-node src/server.ts --watch",
    "build": "tsc",
    "generate-types": "openapi-typescript ./src/openapi/api.schema.yml --output ./src/types/schema.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:watch": "esw --ext .js,.jsx,.ts,.tsx ./src --color --watch",
    "lint:fix": "esw --ext .js,.jsx,.ts,.tsx ./src --color --fix",
    "test": "cross-env OWNER_NAME=XSIGHTME FILE_NAME=data-product-manifest.yaml LOGGER_LOG_LEVEL=error LOGGER_LOG_STYLE=cli NODE_ENV=test jest -i",
    "test:watch": "TESTCONTAINERS_RYUK_DISABLED=true npm test -- --watchAll",
    "watch-node": "nodemon --watch src src/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
